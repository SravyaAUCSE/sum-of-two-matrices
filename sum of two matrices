
#include <stdio.h>

int main() {
   
   int i,j,a[3][3],b[3][3],sum[3][3];
   for(i=0;i<=2;i++)
   {
    for(j=0;j<=2;j++)
    {
    printf("Enter the element a[%d][%d]: ",i,j);
    scanf("%d",&a[i][j]);
    }printf("\n");
   }
   
   for(i=0;i<=2;i++)
   {
    for(j=0;j<=2;j++)
    {
    printf("%d",a[i][j]);    
    }printf("\n");
   }
   
   for(i=0;i<=2;i++)
   {
   for(j=0;j<=2;j++)
   {
    printf("Enter the element b[%d][%d]: ",i,j);
    scanf("%d",&b[i][j]);
   }printf("\n");
   }
   
   for(i=0;i<=2;i++)
   {
    for(j=0;j<=2;j++)
    {
    printf("%d",b[i][j]);    
    }printf("\n");
   }

   
   for(i=0;i<=2;i++)
   {
    for(j=0;j<=2;j++)
    {
    sum[i][j]=a[i][j]+b[i][j];  
    printf("\nthe sum of two matrices is %d:",sum[i][j]);
    }printf("\n");
   }
   
   for(i=0;i<=2;i++)
   {
    for(j=0;j<=2;j++)
    {
     
    printf("%d",sum[i][j]);    
    }printf("\n");
   }
   
    return 0;
}
